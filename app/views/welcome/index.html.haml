#myModal.modal.fade{:role => "dialog"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{"data-dismiss" => "modal", :type => "button"} Ã—
        %h4.modal-title Product details
      .modal-body
        %span
          = image_tag('handi model.jpg', :alt => "rss feed", :class => "img-responsive")
      .modal-footer
        %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} Close


#myCarousel.carousel.slide
  / Indicators
  %ol.carousel-indicators
    %li.active{"data-slide-to" => "0", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "1", "data-target" => "#myCarousel"}
    %li{"data-slide-to" => "2", "data-target" => "#myCarousel"}
  .carousel-inner
    .item.active
      = image_tag('e.jpg', :alt => "rss feed", :class => "img-responsive")
    .item
      = image_tag('epic.jpg', :alt => "rss feed", :class => "img-responsive")
    .item
      = image_tag('idea.jpg', :alt => "rss feed", :class => "img-responsive")
  %a.left.carousel-control{"data-slide" => "prev", :href => "#myCarousel"}
    %span.glyphicon.glyphicon-chevron-left
    %span.sr-only Previous
  %a.right.carousel-control{"data-slide" => "next", :href => "#myCarousel"}
    %span.glyphicon.glyphicon-chevron-right
    %span.sr-only Next
/ /.carousel


